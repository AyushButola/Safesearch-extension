{
  "name": "Safe-search",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Provides a safe browsing experience",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "declarativeNetRequest",
    "activeTab",
    "contextMenus",
    "storage",
    "scripting",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/util.js","src/content.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "src/background.js"
  }
}


